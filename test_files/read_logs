Sub GetProtocolFromRastr()
    Dim Rastr As Object
    Set Rastr = CreateObject("ASTRALib.Rastr")
    
    ' Загружаем файл и выполняем расчет
    Rastr.Load 1, "C:\путь\к\файлу.rg2", "режим"
    Rastr.rgm ""  ' Расчет установившегося режима
    
    ' ПОЛУЧАЕМ ПРОТОКОЛ
    Dim protocol As String
    protocol = Rastr.Protocol
    
    ' Работаем с протоколом (например, сохраняем в файл)
    Dim fileNum As Integer
    fileNum = FreeFile
    Open "C:\temp\protocol.txt" For Output As #fileNum
    Print #fileNum, protocol
    Close #fileNum
    
    ' Или выводим в Immediate Window
    Debug.Print protocol
    
    ' Или ищем нужную информацию
    If InStr(protocol, "Расчет завершен") > 0 Then
        MsgBox "Расчет успешен!"
    End If
    
    Set Rastr = Nothing
End Sub
